{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "name",
    "protocol_version",
    "integration_version",
    "data"
  ],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "com.newrelic.nginx"
      ],
      "pattern": "^(.*)$"
    },
    "protocol_version": {
      "$id": "#/properties/protocol_version",
      "type": "string",
      "title": "The Protocol_version Schema",
      "default": "",
      "examples": [
        "2"
      ],
      "pattern": "^(.*)$"
    },
    "integration_version": {
      "$id": "#/properties/integration_version",
      "type": "string",
      "title": "The Integration_version Schema",
      "default": "",
      "examples": [
        "1.1.0"
      ],
      "pattern": "^(.*)$"
    },
    "data": {
      "$id": "#/properties/data",
      "type": "array",
      "title": "The Data Schema",
      "items": {
        "$id": "#/properties/data/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "metrics",
          "inventory",
          "events"
        ],
        "properties": {
          "metrics": {
            "$id": "#/properties/data/items/properties/metrics",
            "type": "array",
            "title": "The Metrics Schema",
            "items": {
              "$id": "#/properties/data/items/properties/metrics/items",
              "type": "object",
              "title": "The Items Schema",
              "required": [
                "event_type",
                "net.connectionsActive",
                "net.connectionsReading",
                "net.connectionsWaiting",
                "net.connectionsWriting",
                "software.edition",
                "software.version"
              ],
              "properties": {
                "event_type": {
                  "$id": "#/properties/data/items/properties/metrics/items/properties/event_type",
                  "type": "string",
                  "title": "The Event_type Schema",
                  "default": "",
                  "examples": [
                    "NginxSample"
                  ],
                  "pattern": "^(.*)$"
                },
                "net.connectionsActive": {
                  "$id": "#/properties/data/items/properties/metrics/items/properties/net.connectionsActive",
                  "type": "integer",
                  "title": "The Net.connectionsactive Schema",
                  "default": 0,
                  "examples": [
                    1
                  ]
                },
                "net.connectionsReading": {
                  "$id": "#/properties/data/items/properties/metrics/items/properties/net.connectionsReading",
                  "type": "integer",
                  "title": "The Net.connectionsreading Schema",
                  "default": 0,
                  "examples": [
                    0
                  ]
                },
                "net.connectionsWaiting": {
                  "$id": "#/properties/data/items/properties/metrics/items/properties/net.connectionsWaiting",
                  "type": "integer",
                  "title": "The Net.connectionswaiting Schema",
                  "default": 0,
                  "examples": [
                    0
                  ]
                },
                "net.connectionsWriting": {
                  "$id": "#/properties/data/items/properties/metrics/items/properties/net.connectionsWriting",
                  "type": "integer",
                  "title": "The Net.connectionswriting Schema",
                  "default": 0,
                  "examples": [
                    1
                  ]
                },
                "software.edition": {
                  "$id": "#/properties/data/items/properties/metrics/items/properties/software.edition",
                  "type": "string",
                  "title": "The Software.edition Schema",
                  "default": "",
                  "examples": [
                    "open source"
                  ],
                  "pattern": "^(.*)$"
                },
                "software.version": {
                  "$id": "#/properties/data/items/properties/metrics/items/properties/software.version",
                  "type": "string",
                  "title": "The Software.version Schema",
                  "default": "",
                  "examples": [
                    "1.4.6 (Ubuntu)"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            }
          },
          "inventory": {
            "$id": "#/properties/data/items/properties/inventory",
            "type": "object",
            "title": "The Inventory Schema"
          },
          "events": {
            "$id": "#/properties/data/items/properties/events",
            "type": "array",
            "title": "The Events Schema"
          }
        }
      }
    }
  }
}